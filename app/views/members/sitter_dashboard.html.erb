  <div class='col-sm-12'>
    <p>Welcome <%= @member.full_name.titleize %><br />
    </p>
    <p>
      <% if @member.merchant_account_id.nil? %>
        <%= link_to("Add Bank Account", bank_account_member_path(@member), :class => "btn btn-primary") %>
      <% else %>
        <%= link_to("Settle up with a Parent", settle_up_member_path(@member), :class => "btn btn-primary") %>
      <% end %>
    </p>
  </div>
