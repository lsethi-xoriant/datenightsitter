  
  <div class='col-lg-12'>
    <%= @provider.first_name.titleize %>, let's add your banking information so we can deposit money sent to you.
    <br />
    <br />
   </div>
  <div class='col-xs-12 col-lg-4 col-lg-offset-4'>
    <%= form_tag(add_bank_account_member_path(@provider), :method => :post, :class => "form-inline") do %>
      <div class="form-group">
      <%= label_tag(:account_number, "Account Number:") %>
      <%= text_field_tag(:account_number, @account_number, :maxlength => 18) %>
      </div>
      <div class="form-group">
      <%= label_tag(:routing_number, "Routing Number:") %>
      <%= number_field_tag(:routing_number, @routing_number, :maxlength => 9) %>
      </div>
      <div class="form-group">
        <%= check_box_tag(:tos, true, !(@fd.nil?) ) %>
        <%= label_tag(:tos, "I accept the Terms") %>
      </div>
      <div class="row-fluid"><%= submit_tag("Submit", :class=>"btn btn-primary", :disabled => !@fd.nil?) %></div>
    <% end %>
  </div>
